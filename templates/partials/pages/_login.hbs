        <div class="password"><input type="text" placeholder="Adgangskode"><i class="fas fa-lock icon"></i></div>
        <button>Login</button>
        <div>
            <div class="rememberMe">
                <label class="container">Husk mig <input type="checkbox"><span class="checkmark" id="rememberMe"></span> </label>
